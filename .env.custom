
# Docker 镜像配置
DOCKER_IMAGE=ghcr.io/m-longggg/my-custom-argosbx:v1.0
CONTAINER_NAME=my-argosbx-server
HOSTNAME=my-proxy-host

# 协议端口配置
VLPORT=443
VMPORT=8443
HYPORT=2053
TUPORT=2083
XHPORT=2087
VXPORT=2095
ARPORT=2096

# 功能配置
REALITY_DOMAIN=apple.com
WARP_CONFIG=s4x6
NODE_PREFIX=my-proxy
IP_PREFERENCE=4

# Argo 配置（可选）
ENABLE_ARGO=yes
ARGO_DOMAIN=your-domain.argo.com
ARGO_AUTH=your-argo-token
